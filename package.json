{
  "name": "binc",
  "private": true,
  "type": "module",
  "scripts": {
    "create": "tsx scripts/create.ts",
    "commit": "git-cz",
    "dev": "vite",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "pnpm run lint:script && pnpm run lint:style",
    "lint:script": "eslint .",
    "lint:style": "stylelint --fix \"src/**/*.{css,scss}\"",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "changeset": "changeset",
    "ver": "changeset version",
    "release": "changeset publish",
    "alpha": "changeset pre enter alpha",
    "alpha:exit": "changeset pre exit"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^2.0.2",
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.8.0",
    "@inquirer/prompts": "^6.0.1",
    "@storybook/addon-coverage": "^1.0.4",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-interactions": "^8.3.1",
    "@storybook/addon-links": "^8.3.1",
    "@storybook/addon-onboarding": "^8.3.1",
    "@storybook/blocks": "^8.3.1",
    "@storybook/manager-api": "^8.3.1",
    "@storybook/react": "^8.3.1",
    "@storybook/react-vite": "^8.3.1",
    "@storybook/test": "^8.3.1",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/theming": "^8.3.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.0",
    "antd": "^5.20.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "ejs": "^3.1.10",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-storybook": "^0.8.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "storybook": "^8.3.1",
    "stylelint": "^16.8.1",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "tsx": "^4.19.1",
    "type-fest": "^4.24.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.1",
    "vitest": "^2.1.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts}": [
      "npm run lint:script"
    ],
    "**/*.{css,scss}": [
      "npm run lint:style"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
